Host bastion
  Hostname nocturlab.fr
  Port 2222
  User root
  IdentityFile ~/.ssh/id_rsa
  ControlMaster auto
  ControlPath ~/.ssh/ansible-%r@%h:%p
  ControlPersist 5m
 
Host 172.10.0.*
  ProxyCommand ssh -W %h:%p bastion
  IdentityFile ~/.ssh/id_rsa

Host *.lan
  ProxyCommand ssh -W %h:%p bastion
  IdentityFile ~/.ssh/id_rsa

